{
    "name": "heena/otel-bootstrap-php",
    "description": "A zero-configuration for OpenTelemetry in PHP.",
    "type": "library",
    "license": "Apache-2.0",
    "require": {
        "php": "^8.1",
        "open-telemetry/api": "^1.0",
        "open-telemetry/sdk": "^1.0",
        "open-telemetry/exporter-otlp": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Heena\\OpenTelemetry\\": "src/"
        },
        "files": ["src/Resources/autoload.php"]
    },
    "config": {
        "optimize-autoloader": true
    }
}
